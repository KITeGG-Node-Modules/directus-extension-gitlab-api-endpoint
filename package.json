{
	"name": "directus-extension-gitlab-api-endpoint",
	"description": "An endpoint to talk to a GitLab instance through Directus",
	"icon": "extension",
	"version": "1.1.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-endpoint"
	],
	"directus:extension": {
		"type": "endpoint",
		"path": "dist/index.js",
		"source": "src/index.js",
		"host": "^11.3.5"
	},
	"type": "module",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "12.1.4"
	},
	"dependencies": {
		"mime-types": "^2.1.35"
	}
}
