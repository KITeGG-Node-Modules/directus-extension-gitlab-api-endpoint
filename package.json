{
	"name": "kitegg-directus-gitlab-api-endpoint",
	"description": "An enpoint to talk to a GitLab instance trough Directus",
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-endpoint"
	],
	"directus:extension": {
		"type": "endpoint",
		"path": "dist/index.js",
		"source": "src/index.js",
		"host": "^9.24.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "9.24.0"
	},
	"dependencies": {
		"mime-types": "^2.1.35"
	}
}
